{
  "manifest_version": 2,
  "name": "Highfive Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with Highfive.",
  "version": "0.1.8",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "hf_16x16.png",
    "48": "hf_48x48.png",
    "128": "hf_128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://*.highfive.video/*",
          "*://*.highfive.com/*",
          "*://*.fatline.io/*",
          "*://*.highfive-staging.com/*"
      ]
  }
}
